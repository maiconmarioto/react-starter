{
  "name": "@maiconmarioto/react-starter",
  "version": "1.0",
  "description": "Starter kit for react with code splitting using react-router 4",
  "author": "Louis DeScioli <louis.descioli@gmail.com>",
  "repository": "https://github.com/maiconmarioto/react-starter",
  "license": "MIT",
  "scripts": {
    "clear": "rimraf dist",
    "build": "rimraf dist && npm run clear && cross-env NODE_ENV=production webpack -p --env.prod=true",
    "start": "webpack-dev-server --content-base src --hot --inline",
    "analyze": "cross-env NODE_ENV=production webpack-dev-server --env.analyze=true",
    "analyze-prod": "cross-env NODE_ENV=production webpack-dev-server --env.prod=true --env.analyze=true"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-input-range": "^1.1.4",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.7.1",
    "redux-form": "^6.8.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
